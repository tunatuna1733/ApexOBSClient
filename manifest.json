{
    "manifest_version": "1",
    "type": "WebApp",
    "meta": {
        "name": "Apex Legends OBS Overlay",
        "version": "0.0.1",
        "minimum-overwolf-version": "0.125.1.0",
        "author": "Overwolf",
        "icon": "icon.png",
        "icon_gray": "icon_gray.png",
        "description": "Game Events Consumer Sample"
    },
    "permissions": [
        "FileSystem"
    ],
    "data": {
        "game_targeting": {
            "type": "dedicated",
            "game_ids": [
                21566
            ]
        },
        "start_window": "index",
        "windows": {
            "index": {
                "file": "index.html",
                "transparent": true,
                "clickthrough": true,
                "resizable": false,
                "show_in_taskbar": false,
                "size": {
                    "width": 1150,
                    "height": 535
                },
                "start_position": {
                    "Top": 10,
                    "Left": 10
                }
            }
        },
        "game_events": [
            21566
        ],
        "launch_events": [
            {
                "event": "GameLaunch",
                "event_data": {
                    "game_ids": [
                        21566
                    ]
                },
                "start_minimized": false
            }
        ],
        "externally_connectable": {
            "matches": [
                "https://apex-ws.deno.dev"
            ]
        }
    }
}